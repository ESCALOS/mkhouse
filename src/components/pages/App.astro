---
interface Props {
  title: string;
}
import Layout from "@/layouts/Layout.astro";
import MainSlider from "../MainSlider.astro";
import Availability from "../Availability.astro";
import About from "../About.astro";
import GalleryRoomContainer from "../GalleryRoomContainer.astro";
import Services from "../Services.astro";
import Contact from "@/components/Contact.astro";
import MapGoogle from "../MapGoogle.astro";
import Reviews from "../Reviews.astro";
const { title } = Astro.props;
---

<Layout title={title}>
  <MainSlider />
  <Availability />
  <About />
  <GalleryRoomContainer />
  <Services />
  <Reviews />
  <Contact />
  <MapGoogle />
</Layout>
